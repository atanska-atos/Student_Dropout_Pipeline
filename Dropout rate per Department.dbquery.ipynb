   "source": [
    "CREATE OR REPLACE MATERIALIZED VIEW workspace.3_gold_db.mv_droupout_per_department\n",
    "AS SELECT\n",
    "  Department,\n",
    "  COUNT(*) AS Dropout_Count\n",
    "FROM workspace.2_silver_db.students_silver\n",
    "WHERE Dropout = 1\n",
    "GROUP BY Department\n",
    "ORDER BY Dropout_Count DESC"
   ]
